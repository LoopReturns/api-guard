{
    "name": "coliving/api-guard",
    "description": "A simple way of authenticating your APIs with API keys using Laravel",
    "keywords": [
        "api",
        "rest",
        "json",
        "api keys",
        "api authentication",
        "laravel"
    ],
    "homepage": "https://github.com/dungnh/api-guard",
    "authors": [
        {
            "name": "Dzung Nguyen",
            "email": "dungnh@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/support": "^5.3.0 | ~6.0| ~6.1| ~6.2| ~6.3",
        "illuminate/database": "^5.3.0 | ~6.0| ~6.1| ~6.2| ~6.3",
        "ellipsesynergie/api-response": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0 || ^5.0 || ^7.0"
    },
    "autoload": {
        "psr-4": {
            "Chrisbjr\\ApiGuard\\": "src"
        }
    },
    "minimum-stability": "stable"
}
